    <script type="text/javascript">
        function showNotification(message, type) {
            var notification;
            if (type == 'failed' || type == 'error')
                notification = noty({ text: message, type: 'error' });
            else if (type == 'success' || type == 'ok')
                notification = noty({ text: message, type: 'success' });
            else if (type == 'cancel')
                notification = noty({ text: message, type: 'warning' });
            else if (type == 'info' || type == 'information')
                notification = noty({ text: message, type: 'information' });

            return notification;
        }
        function showConfirmDialog(message, okButtonText, cancelButtonText, okClickFunction, cancelClickFunction) {
            var notification;
            notification = noty(
            {
                text: message,
                type: 'alert',
                layout: 'center',
                buttons: [
                    { addClass: 'btn btn-primary', text: okButtonText, onClick: okClickFunction },
                    { addClass: 'btn btn-danger', text: cancelButtonText, onClick: cancelClickFunction }
                ]
            });

            return notification;
        }

        function clearCache() {
            $.get('@Url.Content("~/Management/ClearCache/")', function (data) {
                showNotification(data);
            }); 

            return false;
        }

        function padLeft(text, character, length) {
            return (text + "").length >= length ? (text + "") : padLeft(character + (text + ""), character, length);
        }

        function padRight(text, character, length) {
            return (text + "").length >= length ? (text + "") : padRight((text + "") + character, character, length);
        }
    </script>

